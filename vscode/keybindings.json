[
   {
      "key": "ctrl+meta+left",
      "command": "workbench.action.moveEditorToLeftGroup",
      "when": "editorFocus"
   },
   {
      "key": "ctrl+meta+right",
      "command": "workbench.action.moveEditorToRightGroup",
      "when": "editorFocus"
   },
   {
      "key": "ctrl+[Backquote]",
      "command": "workbench.action.focusNextGroup",
      "when": "editorFocus"
   },
   {
      "key": "shift+alt+t",
      "command": "editor.action.inspectTMScopes"
   },
   {
      "key": "alt+v",
      "command": "toggleVim"
   },
]
